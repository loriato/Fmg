@using Europa.Resources
@using Europa.Extensions
@model Tenda.EmpresaVenda.ApiService.Models.BreveLancamento.BreveLancamentoDto

<script type="text/javascript" src="~/Static/projeto/consulta-estoque/modal-empreendimento.js"></script>

<div class="modal fade " id="disponibilidade-empreendimento-modal" tabindex="-1" role="dialog" data-backdrop="static">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myModalLabel">
                    @(Model.Empreendimento.IsNull()?
                    GlobalMessages.ConsultaEstoqueEmpreendimento :
                    GlobalMessages.ConsultaEstoqueEmpreendimento + " - " + Model.Empreendimento.Nome)
                </h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body">
                @Html.HiddenFor(x => x.Empreendimento.Divisao)
                @{
                    Html.RenderPartial("~/Views/ConsultaEstoque/_EmpreendimentoDatatable.cshtml");
                }
            </div>
            <div class="modal-footer">
                <button type="button" class="button__red__outline" title="@GlobalMessages.Fechar" data-dismiss="modal">
                    @GlobalMessages.Fechar
                </button>
            </div>
        </div>
    </div>
</div>