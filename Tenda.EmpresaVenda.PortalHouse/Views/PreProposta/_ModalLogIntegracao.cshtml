@using Europa.Resources

<script src="~/Static/projeto/pre-proposta/modal-integracao-log.js"></script>

<script>
    Europa.Components.LogIntegracaoModal.UrlListar = '@Url.Action("ListarLogIntegracao", "PreProposta")';
</script>

<div class="modal fade bd-example-modal-lg" id="log-integracao-modal" tabindex="-1" role="dialog" data-backdrop="static">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myModalLabel">@GlobalMessages.LogDeIntegracao</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body">
                <div class="col-md-12">
                    <div ng-controller="logIntegracao as table">
                        <table datatable="" dt-options="table.dtOptions" dt-columns="table.dtColumns"
                               dt-instance="table.dtInstance" class="table table-striped table-bordered"
                               dt-column-defs="table.dtColumnDefs"></table>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="button__red__outline" title="@GlobalMessages.Fechar" data-dismiss="modal">
                    @GlobalMessages.Fechar
                </button>
            </div>
        </div>
    </div>
</div>
