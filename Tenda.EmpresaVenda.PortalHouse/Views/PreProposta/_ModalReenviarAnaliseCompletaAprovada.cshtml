@using Europa.Resources

<script type="text/javascript" src="~/Static/projeto/pre-proposta/modal-reenviar-analise-completa-aprovada.js"></script>

<div class="modal fade" id="reenviar-analise-completa-aprovada-modal" tabindex="-1" role="dialog" data-backdrop="static">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myModalLabel">@GlobalMessages.AvancarEtapa</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12 form-group">
                        @Html.Label(string.Format(GlobalMessages.MsgConfirmacaoRetrocederPreProposta2, GlobalMessages.SituacaoProposta_AnaliseCompletaAprovada), new { @class = "control-label" })
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12 form-group">
                        @Html.Label(GlobalMessages.Justificativa, new { @class = "control-label"})
                        @Html.TextArea("justificativa", new { @class = "form-control"})
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button id="btn_confirmar_envio_analise" type="button" class="btn button__red__outline" title="@GlobalMessages.Confirmar" onclick="Europa.Components.ModalReenviarAnaliseCompletaAprovada.ReenviarWorkflow()" disabled>
                    @GlobalMessages.Confirmar
                </button>
                <button type="button" class="button__red__outline" title="@GlobalMessages.Fechar" data-dismiss="modal">
                    @GlobalMessages.Fechar
                </button>
            </div>
        </div>
    </div>
</div>
