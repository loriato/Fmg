@using Europa.Resources
@using Europa.Extensions
@model Tenda.EmpresaVenda.Portal.Models.PreCadastroDTO

<div class="col-md-24">
    <br />
    <div class="geenFieldSet">
        <div class="panel-heading">
            <div>
                <h3 class="panel-title pull-left">@GlobalMessages.DadosTributarios</h3>
                <div class="pull-right">
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>
</div>
<div class="col-md-24">
    <form id="form_dados_tributarios">
        <div class="col-md-4 form-group">
            @Html.Label(GlobalMessages.LegislacaoFederal, new { @class = "control-label" })
            @Html.TextBoxFor(x => x.EmpresaVenda.LegislacaoFederal, new { @class = "form-control", @maxlength = 30 })
        </div>
        <div class="col-md-4 col-md-offset-1 form-group">
            @Html.Label(GlobalMessages.Simples + " *", new { @class = "control-label" })
            @Html.TextBoxFor(x => x.EmpresaVenda.Simples, new { @class = "form-control", @maxlength = 30 })
        </div>
        <div class="col-md-4 col-md-offset-1 form-group">
            @Html.Label(GlobalMessages.Simei + " *", new { @class = "control-label" })
            @Html.TextBoxFor(x => x.EmpresaVenda.SIMEI, new { @class = "form-control", @maxlength = 30 })
        </div>
        <div class="col-md-4 col-md-offset-1 form-group">
            @Html.Label(GlobalMessages.LucroPresumido + " *", new { @class = "control-label" })
            @Html.TextBoxFor(x => x.EmpresaVenda.LucroPresumido, new { @class = "form-control", maxlength = 13, Value = "0,00", @style = "text-align: right" })
        </div>
        <div class="col-md-4 col-md-offset-1 form-group">
            @Html.Label(GlobalMessages.LucroReal + " *", new { @class = "control-label" })
            @Html.TextBoxFor(x => x.EmpresaVenda.LucroReal, new { @class = "form-control", maxlength = 13, Value = "0,00", @style = "text-align: right" })
        </div>
    </form>
</div>


