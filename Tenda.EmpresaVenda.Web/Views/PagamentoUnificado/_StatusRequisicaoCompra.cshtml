@using Europa.Resources

<div class="modal" id="PagamentoUnificado" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" data-backdrop="static">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" onclick="Europa.Controllers.PagamentoUnificado.HidePagamentoUnificado()" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">@GlobalMessages.StatusRequisicaoCompra</h4>
            </div>
            <div class="modal-body">
                <div class="col-md-24">
                    <div class="col-md-24">@Html.Label(GlobalMessages.Status)</div>
                    <div class="col-md-24" id="PagamentoUnificado_Status" style="height:30px">

                    </div>
                    <div class="col-md-24">
                        @Html.Label(GlobalMessages.Detalhes)
                    </div>
                    <div class="col-md-24 panel panel-default" id="PagamentoUnificado_Log" style="height: 300px;max-height: 300px; overflow-x: auto;padding: 5px;">

                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" style="display:none" id="PagamentoUnificado_Download" onclick="Europa.Controllers.PagamentoUnificado.DownloadArquivo()"><i class="fa fa-download" aria-hidden="true"></i> @GlobalMessages.Download</button>
                <button type="button" class="btn btn-default" onclick="Europa.Controllers.PagamentoUnificado.HidePagamentoUnificado()"><i class="fa fa-ban" aria-hidden="true"></i> @GlobalMessages.Fechar</button>
            </div>
        </div>
    </div>
</div>
<form id="PagamentoUnificado_Exportar" method="POST"></form>
