@using System
@using Europa.Resources
@using Europa.Resources
@using Tenda.EmpresaVenda.Web.Models.Application

<div class="col-md-24">
    <div ng-controller="ConsultaAceitesContratosDatatable as aceites">
        <div id="datatable_header" class="col-md-24">
            <div class="pull-right">
                    <button type="button" title="@GlobalMessages.ExportarPagina" class="btn btn-default" onclick="Europa.Controllers.ConsultaAceitesContratos.ExportarPagina()">
                        <i class="fa fa-file-excel-o"></i> @GlobalMessages.ExportarPagina
                    </button>
                    <button type="button" title="@GlobalMessages.ExportarTodos" class="btn btn-default" onclick="Europa.Controllers.ConsultaAceitesContratos.ExportarTodos()">
                        <i class="fa fa-file-excel-o"></i> @GlobalMessages.ExportarTodos
                    </button>
            </div>
        </div>
        <table datatable="" dt-options="aceites.dtOptions" dt-columns="aceites.dtColumns"
               dt-instance="aceites.dtInstance" class="table table-striped table-bordered"
               dt-column-defs="aceites.dtColumnDefs"></table>
    </div>
</div>