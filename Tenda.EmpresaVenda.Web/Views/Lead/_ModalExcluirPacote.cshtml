@using Europa.Resources
@using Tenda.Domain.EmpresaVenda.Enums

<div class="modal" id="modal_excluir_pacote" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" data-backdrop="static">
    <div class="modal-dialog" role="document"style="width: 500px;">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">@GlobalMessages.Excluir @GlobalMessages.Pacote</h4>
            </div>
            <div class="modal-body">
                <form id="form_excluir_pacote">
                    <div class="col-md-24">
                        @Html.Label(GlobalMessages.Pacote, new { @class = "control-label" })
                        <select id="autocomplete_pacote_excluir" multiple="multiple" name="autocomplete_pacote_excluir" class="select2-container form-control"></select>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button  class="btn btn-default" type="button" onclick="Europa.Controllers.Lead.ExcluirPacote()">
                    <i class="fa fa-trash"></i> @GlobalMessages.Excluir
                </button>
                <button class="btn btn-default" type="button" data-dismiss="modal">
                    <i class="fa fa-close"></i> @GlobalMessages.Fechar
                </button>
            </div>
        </div>
    </div>
</div>
